{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
      "no-trailing-whitespace": false,
      "whitespace": false,
      "typedef-whitespace": false,
      "no-inferrable-types": false,
      "max-line-length": false,
      "no-empty-interface": false

    }
}
